package application;

import entities.Account;
import entities.BusinessAccount;
import entities.SavingsAccount;

public class Program {

	public static void main(String[] args) {
		Account acc = new Account(1001, "Fred", 0.0);
		BusinessAccount bacc = new BusinessAccount(2001, "Camila", 0.0, 500.00);
		SavingsAccount sacc = new SavingsAccount(3001, "Flavia", 50.0, 0.5);
		
		//UPCASTING
		Account acc1 = bacc;
		Account acc2 = sacc;
		
	}

}
